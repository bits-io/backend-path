
<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="nav-home-tab">
    <!-- DataTales Example -->
     <div class="row">
        <% for(let x = 0; x < item.imageId.length; x++){ %>
        <div class="col-md-4">
            <div class="card shadow mb-4 mt-2"> 
                <div class="card-body">
                       <img src="http://localhost:3000/<%= item.imageId[x].imageUrl %>" alt="image">
                </div>
            </div>
        </div>
        <% } %>
     </div>
     <!-- End Table -->
     <a href="/admin/item" class="btn btn-warning">Back</a>
</div>